}

/* fsa */
.neuton h1, .neuton h2, .neuton h3, .neuton h4, .neuton h5, .neuton h6 {
  font-weight: 300; }

/* Accent text
------------------------------------------------*/
.accent-text {
  font-family: {{ settings.accent-font }};
  text-transform: uppercase; }

/* fsa */
.news-cycle .accent-text {
  letter-spacing: 0.100em; }

/* fsa */
.pt-sans .accent-text {
  letter-spacing: 0.100em;
