  border-radius: 4px;
  border: 0;
  outline: 0;
  padding: 0.750em 0.875em;
  font-size: 0.813em;
  -webkit-font-smoothing: antialiased; }

.action-btn.disabled, .action-btn.disabled:hover {
  cursor: default !important;
  background-color: {{ settings.submit-button-colour }} !important; }

.action-btn:hover {
  background-color: {{ settings.submit-button-hover-colour }}; }

.action-btn.small {
  padding: 0.750em 0.813em 0.500em 0.875em;
  font-size: 0.625em; }

/* fsa */
.news-cycle .action-btn {
  padding: 0.563em 0.875em 0.750em;
