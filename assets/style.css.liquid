/* General */
/* Icon font
------------------------------------------------*/
@font-face {
  font-family: 'editions';
  src: url('{{ "editions-icons.eot" | asset_url }}');
  src: url('{{ "editions-icons.eot" | asset_url }}?#iefix') format("embedded-opentype"), url('{{ "editions-icons.woff" | asset_url }}') format("woff"), url('{{ "editions-icons.ttf" | asset_url }}') format("truetype"), url('{{ "editions-icons.svg" | asset_url }}#editions') format("svg");
  font-weight: normal;
  font-style: normal; }

body {
  font-size: 1.125em;
  line-height: 1.5;
  font-family: {{ settings.text-font }};
  /*-webkit-font-smoothing: antialiased;*/
  color: {{ settings.text-colour }};
  text-align: center;
  background-color: {{ settings.background-colour }};
  background-image: {% if settings.use-background-image %} url('{{ "background-image.png" | asset_url }}') {% endif %};
  background-repeat: {{ settings.repeat-background }};
  background-position: {{ settings.background-position }};
  background-attachment: {% if settings.fixed-background == true %} fixed {% endif %}; }

#page-wrap, .bold #footer-tools, .bold #payment-options {
  width: 940px;
  margin: 0 auto 1.500em;
  padding: 0 10px; }

/* ts:b */
.bold #page-wrap {
  margin-bottom: 0; }

::-moz-selection {
  background:  {{ settings.text-highlight-colour }} ;
  color: #fff;
  text-shadow: none; }

::selection {
  background:  {{ settings.text-highlight-colour }} ;
  color: #fff;
  text-shadow: none; }

a, a:visited {
  color: {{ settings.link-colour }}; }

a:hover {
  color: {{ settings.link-hover-colour }}; }

h1, h2, h3, h4, h5, h6 {
