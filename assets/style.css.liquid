/* Heading text
------------------------------------------------*/
.heading-text {
  font-family: {{ settings.heading-font }};
  /*-webkit-font-smoothing: antialiased;*/
}

/* Rich text editor
------------------------------------------------*/
.rte p, .rte ol, .rte ul, .rte blockquote {
  width: 72.3%;
  text-align: left;
  margin: 0px auto 20px;
  font-size: 0.875em; }

.rte h1, .rte h2, .rte h3, .rte h4, .rte h5, .rte h6 {
  width: 72.3%;
  text-align: left;
  margin: 0px auto 20px;
  font-weight: 400;
  color:  {{ settings.text-colour }} ; }

/* fsa */
.neuton .rte p, .neuton .rte ol, .neuton .rte ul, .neuton .rte blockquote {
  font-size: 1.000em;
  font-weight: 300; }

/* fsa */
.neuton .rte blockquote {
  -webkit-font-smoothing: antialiased;
  font-size: 1.125em; }

/* fsa */
.neuton .rte i, .neuton .rte b i, .neuton .rte em {
  font-size: 0.875em;
  line-height: 1.4; }

.rte ol, .rte ul, .rte blockquote {
  padding-left: 6%;
  width: 74%; }

.rte ol {
  list-style: decimal; }

.rte li {
  margin-bottom: 8px; }

.rte ul {
  list-style: none;
  padding-left: 4%; }

/* ie7 fix */
.lt-ie8 .rte ul {
  list-style: disc;
  padding-left: 6%; }

.rte ul li {
  position: relative; }

.rte ul li:before {
  content: '\2022';
  position: absolute;
  left: -12px;
  top: -1px;
  font-size: 1.100em;
  opacity: 0.5;
  zoom: 1;
  filter: alpha(opacity=50); }

.rte blockquote {
  margin-top: 2.250em;
  font-style: italic;
  position: relative;
  font-size: 1.000em;
  opacity: 0.8;
  zoom: 1;
  filter: alpha(opacity=80);
  -webkit-font-smoothing: antialiased; }

.lt-ie9 .rte blockquote {
  background-color:  {{ settings.background-colour }} ; }

.rte blockquote:before {
  content: '\201C';
  font-size: 4.500em;
  position: absolute;
  top: -10px;
  left: 15px;
  line-height: 52px;
  opacity: 1;
  display: block;
  height: 32px;
  width: 32px;
  text-indent: -8px;
  opacity: 0.15;
  zoom: 1;
  filter: alpha(opacity=15); }

.lt-ie9 .rte blockquote:before {
  background-color:  {{ settings.background-colour }} ;
  top: 0px; }

.rte img, .rte-image-wrap {
  max-width: 100%; }

.rte p > img {
  display: none; }

p.rte-image-wrap {
  width: auto;
  display: inline-block;
  zoom: 1;
  /* ie7 fix */
  *display: inline;
  /* ie7 fix */ }

/* fsa */
.pt-serif p.rte-image-wrap {
  margin-bottom: 22px; }

.rte b, .rte strong {
  -webkit-font-smoothing: antialiased; }

.rte > *:last-child {
  margin-bottom: 12px; }

.rte iframe {
  max-width: 100%; }

/* Borders
------------------------------------------------*/
.double-borders {
  border-top: 1px solid {{ settings.primary-border-colour }};
  margin: 4px 0px 0px; }

.double-borders:before {
  content: '';
  display: block;
  position: relative;
  top: -5px;
  left: 0;
  right: 0;
  height: 1px;
  background-color: {{ settings.primary-border-colour }}; }

.single-borders {
  border-top: 1px solid {{ settings.primary-border-colour }};
  margin: 0px 0px 0px; }

.drawn-borders {
  border-image: url({{ "dotted-divider.png" | asset_url }}) 20 10 repeat;
  border-bottom: 0;
  border-left: 0;
  border-right: 0;
}

/* Buttons
------------------------------------------------*/
.action-btn {
  display: inline-block;
  zoom: 1;
  *display: inline;
  background-color: {{ settings.submit-button-colour }};
  color: {{ settings.submit-button-text-colour }};
