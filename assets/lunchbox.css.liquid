@font-face {
  font-family: 'Lunchbox';
  src: url('{{ "lunchbox_light-webfont.eot" | asset_url }}');
  src: url('{{ "lunchbox_light-webfont.eot" | asset_url }}?#iefix') format("embedded-opentype"),
       url('{{ "lunchbox_light-webfont.woff" | asset_url }}') format("woff"),
       url('{{ "lunchbox_light-webfont.ttf" | asset_url }}') format("truetype"),
       url('{{ "lunchbox_light-webfont.svg" | asset_url }}#lunchbox_light') format("svg");
  font-weight: 300;
  font-style: normal; }
  
